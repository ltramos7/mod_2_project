<% provide(:title, "Edit user") %> <h1>Update your profile</h1>

<%= form_with model: @user do |f| %>
  

<%= f.label :"First Name" %>
<%= f.text_field :first_name %>
<%= f.label :"Last Name" %>
<%= f.text_field :last_name %>
<%= f.label :"User Name" %>
<%= f.text_field :user_name %>
<%= f.submit "Save changes" %>


<% end %>
